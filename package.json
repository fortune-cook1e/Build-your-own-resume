{
  "name": "resume-generator",
  "version": "1.0.0",
  "description": "Build your own resume online",
  "private": true,
  "scripts": {
    "dev": "pnpm run --parallel -r dev",
    "tsc": "pnpm run --parallel -r tsc",
    "lint": "pnpm run --parallel -r lint",
    "format": "prettier --write \"apps/**/src/**/*.{ts,tsx}\" \"apps/**/test/*.{ts,tsx}\"",
    "build": "pnpm run -r build",
    "build:shared": "pnpm --filter shared run build",
    "build:server": "pnpm --filter server run build",
    "build:web": "pnpm --filter web run build",
    "build:board": "pnpm --filter board run build",
    "prisma:seed": "pnpm --filter server run prisma:seed",
    "prisma:generate": "pnpm --filter server prisma:generate",
    "prisma:deploy": "pnpm --filter server prisma:deploy",
    "prisma:migrate:dev": "pnpm --filter server prisma:migrate:dev",
    "prisma:studio": "pnpm --filter server prisma:studio",
    "test": "pnpm run -r test",
    "test:web": "pnpm --filter web run test",
    "test:server": "pnpm --filter server run test"
  },
  "keywords": [
    "Resume Generator",
    "Resume"
  ],
  "author": "fortune cookie",
  "license": "ISC",
  "dependencies": {
    "@chakra-ui/next-js": "^2.2.0",
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@phosphor-icons/react": "^2.0.15",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "axios": "^1.6.7",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.10",
    "framer-motion": "^11.0.8",
    "immer": "^10.0.3",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-zoom-pan-pinch": "^3.4.3",
    "sharp": "^0.33.3",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "webfontloader": "^1.6.28",
    "zod": "^3.22.4",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.24",
    "@types/react": "18.2.65",
    "@types/react-dom": "^18.2.65",
    "@types/webfontloader": "^1.6.38",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.2.13",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  }
}
